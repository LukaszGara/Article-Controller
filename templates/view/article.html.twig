{% extends 'base.html.twig' %}

{% block title %} cms {% endblock %}

{% block body %}
<div class="container">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">{{ article.title }}</h4>
            <h6 class="card-subtitle mb-2 text-muted">{{ article.author }}</h6>
            <p class="card-text">{{ article.body }}</p>
        </div>
    </div>
</div>
{% endblock %}